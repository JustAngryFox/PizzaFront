button>\r\n</form> \r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\n\r\nexport default {\r\n\r\nname: 'AddPizzaView',\r\n\t\r\n  \r\n  methods:{\r\nSendPizza(){\r\nvar formData = new FormData(document.forms.PizzaForm)\r\nformData.append('name','govno');\r\nformData.append('price','govno');\r\nformData.append('description','govno');\r\nconsole.log(formData)\r\naxios.post('http://testpizzabackend/api/upload_product', formData, {\r\nheaders: {\r\n'Content-Type': 'multipart/form-data'\r\n}\r\n}).then(response => {\r\nconsole.log(\"response\", response.data)\r\n})\r\n}\r\n}\r\n  \r\n  \r\n}\r\n</script>\r\n\r\n<style scope>\r\n#AddPizzaView{margin:50px 0px 0px 0px;background:#343a40;border-radius:5px;padding:30px 0px;}\r\n</style>"]},"metadata":{},"sourceType":"module"}